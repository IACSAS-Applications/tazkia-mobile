{
  "expo": {
    "name": "Manhaj Nabawi|منهج نبوي",
    "slug": "manhaj-nabawi",
    "version": "1.0.0",
    "scheme": "manhaj-nabawi",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e4f1ea"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.iacsas.manhaj"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e4f1ea"
      },
      "package": "com.iacsas.manhaj"
    },
    "extra": {
      "eas": {
        "projectId": "5747c038-a916-4908-af57-c433fcae6460"
      }
    },
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      "expo-localization"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cca9d80e-5828-4dd6-b37f-79284f6d4bbf"
    }
  }
}
